    <script>
        preIndex = '';
        $('.menu-ls').click(function () {
            $('.active').removeClass('active');
            preIndex = "#"+$(this).data("preid");
            var controller = $(this).data("controller");
            var action = $(this).data("action");
            var url = '@Url.Action("Action", "Controller")';
            url = url.replace('Controller', controller).replace('Action', action);
            if ($('#' + $(this).data("menu")).length == 0) {

                $('#menu-nav').append('<li class="active toptabs" data-preid="' + preIndex + '">' +
                    '<a href="#' + $(this).data("menu") + '" data-toggle="tab">' + $(this).data("menuname") + '&nbsp;&nbsp;<span class="tabClose"><i class="fa fa-times" aria-hidden="true"></i></span></a>' +
                    '</li>');
                $('#menu-tab').append('<div class="tab-pane active" id="' + $(this).data("menu") + '"></div>');
                $(".loader").show();
                $('#' + $(this).data("menu")).load(url, function () {
                    $(".loader").hide();
                });
            }
            $("a[href='#" + $(this).data("menu") + "']").tab('show');

        });


        $(document).on('click', '.toptabs', function () {
            preIndex = $(this).data("preid");
        });
    </script>

