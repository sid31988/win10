@model Win10WebApp.ViewModels.BulkPurchaseViewModel
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-info">

            <div class="panel-body">
                <div id="forexMasterRoot" class="row page-root">

                    <!--Form Design-->
                    <div id="forexform" class="floating-form col-sm-12">
                        @{Html.RenderPartial("~/Views/BulkPurchase/_forexform.cshtml", Model);}
                    </div>
                    
                    
                        <!--View Design-->
                        <div id="divDatatable" class="col-sm-6 grid-control">
                            @{Html.RenderPartial("~/Views/BulkPurchase/_forexdatatable.cshtml", Model.BulkPurchaseForex);}
                        </div>

                        <!--Details Design-->
                        <div id="forexSummary" class="details-panel editor-control col-sm-6">
                            @{Html.RenderPartial("~/Views/BulkPurchase/_forexsummary.cshtml", Model);}
                        </div>
                   
                    
                </div>
            </div>
        </div>
    </div>
</div>