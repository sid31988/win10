@model Win10WebApp.Models.ReceiveTrustMaster
@{
    Layout = null;
}

<input type="text" name="Id" value="@Model.Id" style="display: none;" />
<div class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-sm-2" for="date">Date</label>
        <div class="col-sm-5">
            <input type="text" id="date" name="date" value="@Model.Date"
                   class="form-control" placeholder=" " onclick="(this.type='date')">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="issuer">Issuer</label>
        <div class="col-sm-5">
            <input type="text" id="issuer" name="issuer" value="@Model.Issuer"
                   class="form-control" placeholder=" ">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="curr">Curr</label>
        <div class="col-sm-5">
            <input type="text" id="curr" name="curr" value="@Model.Curr"
                   class="form-control" placeholder=" ">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="prefix">Prefix</label>
        <div class="col-sm-5">
            <input type="text" id="prefix" name="prefix" value="@Model.Prefix"
                   class="form-control" placeholder=" ">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="startno">Start No</label>
        <div class="col-sm-5">
            <input type="text" id="startno" name="startno" value="@Model.StartNo"
                   class="form-control" placeholder=" " onclick="(this.type='number')">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="nos">Nos</label>
        <div class="col-sm-5">
            <input type="text" id="nos" name="nos" value="@Model.Nos"
                   class="form-control" placeholder=" " onclick="(this.type='number')">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="endno">End No</label>
        <div class="col-sm-5">
            <input type="text" id="endno" name="endno" value="@Model.EndNo"
                   class="form-control" placeholder=" " onclick="(this.type='number')">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="deno">Deno</label>
        <div class="col-sm-5">
            <input type="text" id="deno" name="deno" value="@Model.Deno"
                   class="form-control" placeholder=" " onclick="(this.type='number')">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-2" for="value">Value</label>
        <div class="col-sm-5">
            <input type="text" id="value" name="value" value="@Model.Value"
                   class="form-control" placeholder=" " onclick="(this.type='number')">
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-5">
            <button type="button" class="btn btn-success" id="btnSaveAct">Save</button>
            <button type="button" class="btn btn-danger" id="btnCancelAct">Cancel</button>
        </div>
    </div>


</div>


